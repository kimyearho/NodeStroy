<!DOCTYPE html>
<html lang="kr" ng-app="nodeapp">

<head>

    <base href="/">

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

    <link rel="image_src" href="http://nodestory.com/images/dk.jpg" />

    <title>NodeStory 블로그</title>

    <!-- Bootstrap core CSS -->
    <link href="assets/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="assets/css/styles.css" rel="stylesheet">

    <link href="assets/css/chat1.css" rel="stylesheet">

    <!-- animate -->
    <link href="/node_modules/angular-ui-router-anim-in-out/css/anim-in-out.css" rel="stylesheet">

</head>

<body>
<div id="fb-root"></div>
<script>
    window.fbAsyncInit = function() {
        FB.init({appId: '186709768436817', status: true, cookie: true,
            xfbml: true});
    };
    (function() {
        var e = document.createElement('script'); e.async = true;
        e.src = document.location.protocol +
                '//connect.facebook.net/ko_KR/all.js';
        document.getElementById('fb-root').appendChild(e);
    }());
</script>

<!-- Navmenu -->
<navmenu></navmenu>

<!-- Content -->
<section class="content bg-black-3">

    <!-- Container -->
    <div class="container">

        <!-- Row -->
        <div class="row">

            <!-- Main Col -->
            <div class="col-md-8 main-col">
                <div ui-view class="anim-in-out anim-fade" data-anim-speed="300"></div>
            </div>
            <!-- /Main Col -->

            <!-- Side Col -->
            <div class="col-md-4 side-col">

                <!-- inner row -->
                <div class="row">

                    <!-- col -->
                    <div class="col-sm-6 col-md-12">

                        <!-- unicard -->
                        <div class="unicard unicard-framed pad-20 side-widget mgb-30 hidden-xs">
                            <!-- post img -->
                            <div class="post-img">
                                <a class="img-link"><img src="images/dk.jpg" alt="" /></a>
                                <div class="unicaption pad-20 img-layer grad-caption">
                                    <h4 class="unicaption-title fw-bold case-u"><a>벌써 연말이야...</a></h4>
                                </div>
                            </div>
                            <!-- /post img -->
                        </div>
                        <!-- /unicard -->

                    </div>
                    <!-- /col -->

                    <!-- APlayer -->
                    <div class="col-sm-6 col-md-12">
                        <div class="unicard unicard-framed pad-20 side-widget mgb-30">
                            <h4 class="case-u fw-bold fg-primary">music</h4>
                            <div id="player4" class="aplayer"></div>
                        </div>
                    </div>

                    <!-- widget -->
                    <div class="col-sm-6 col-md-12">
                        <div class="unicard unicard-framed side-widget mgb-30" style="padding: 20px;">
                            <h4 class="case-u fw-bold fg-primary">widget</h4>
                            <ul class="unimedia-list post-list-sm">
                                <li>
                                    <div wl-ng-weather theme="widget1" background="#244872" location="seoul"></div>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <!-- /widget -->

                    <!-- popular List -->
                    <div class="col-sm-6 col-md-12">
                        <div class="unicard unicard-framed pad-20 side-widget mgb-30">
                            <h4 class="case-u fw-bold fg-primary">Simple Chat Beta0.1</h4>
                            <simplechat></simplechat>
                        </div>
                    </div>
                    <!-- /col -->

                    <!-- popular List -->
                    <div class="col-sm-6 col-md-12">
                        <div class="unicard unicard-framed pad-20 side-widget mgb-30">
                            <popular></popular>
                        </div>
                    </div>
                    <!-- /col -->

                    <!-- social Link -->
                    <div class="col-sm-6 col-md-12">
                        <div class="unicard unicard-framed pad-20 side-widget mgb-30">
                            <h4 class="case-u fw-bold fg-primary">Link</h4>
                            <social></social>
                        </div>
                    </div>
                    <!-- /col -->
                </div>
                <!-- /inner row -->
            </div>
            <!-- /Side Col -->
        </div>
        <!-- /Row -->
    </div>
    <!-- /Container -->
</section>
<!-- /Content -->

<footers></footers>

<input type="hidden" id="nickname" value="" />

