<!-- ==================== Common Lib ====================  -->

<!-- jQuery Scripts -->
<script defer src="assets/js/jquery-latest.min.js"></script>

<!-- Bootstrap core JS -->
<script defer src="assets/bootstrap/js/bootstrap.min.js"></script>

<!-- ==================== Core Lib ====================  -->

<!-- AngularJS Module Scripts -->
<script defer src="assets/js/angular/angular.min.js"></script>
<script defer src="assets/js/angular/angular-ui-router.min.js"></script>
<script defer src="assets/js/angular/angular-animate.min.js"></script>

<!-- Controller Script -->
<script defer src="post/list/MainController.js"></script>
<script defer src="post/list/NewsController.js"></script>
<script defer src="post/list/GameController.js"></script>
<script defer src="post/list/ITController.js"></script>
<script defer src="post/content/ViewController.js"></script>

<!-- Angular Core Script -->
<script defer src="core.js"></script>

<!-- ==================== 3rd party Lib ====================  -->

<!-- Socket Module Script -->
<script defer src="assets/js/socket.io.min.js"></script>

<!-- APlayer -->
<script defer src='/node_modules/aplayer/dist/APlayer.min.js'></script>
<script defer src='assets/js/player.js'></script>

<!-- Google Map -->
<script defer src='https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyDLjMdWtsOTMiF-hIdMraLG3bVFumUfnWM'></script>

<!-- Weather -->
<script defer src='assets/js/weather/wl-ng-weather.min.js'></script>
<script defer src='assets/js/weather/skycons.min.js'></script>

<!-- Script -->
<script defer src='assets/js/masonry.pkgd.min.js'></script>
<script defer src="assets/js/uikit.js"></script>

<script defer src='/node_modules/angular-ui-router-anim-in-out/anim-in-out.js'></script>
<script defer src="assets/js/angular-pagination.js"></script>

</body>
</html>